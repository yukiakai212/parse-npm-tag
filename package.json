{
  "name": "publish-tag",
  "version": "1.0.0",
  "description": "Extracts semver version and npm publish tag from GitHub tag",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npx vitest run --reporter verbose",
    "format": "prettier --write **/*.js",
    "format:check": "prettier --check **/*.js",
    "lint": "eslint src/**/*.js",
    "build": "npx tsup"
  },
  "keywords": [
    "parse-npm-tag",
    "publish-tag",
    "tag",
    "parse"
  ],
  "author": "Yuki_akai",
  "homepage": "https://github.com/yukiakai212/publish-tag",
  "engines": {
    "node": ">= 16"
  },
  "maintainers": [
    {
      "name": "Yuki_akai",
      "homepage": "https://github.com/yukiakai212"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/publish-tag.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/publish-tag/issues"
  },
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "eslint": "^9.30.0",
    "eslint-plugin-n": "^17.20.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
